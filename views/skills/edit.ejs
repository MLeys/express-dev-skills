<%- include("../partials/headerStyle.ejs") %>

<title>Coding Skills - edit</title>
</head>
<body>



    
    <h1>Edit Skill</h1>

    <form action="/skills/<%= skill.skill %>?_method=PUT" method="POST">
        <input type="text" name="skill" value="<%= skill.skill %> ">
        <input type="checkbox" 
        <%  if (skill.done) { %>
            checked  
            <% } %> 
        name="done">
        <button type="submit">Save Skill</button>
    </form>
    

