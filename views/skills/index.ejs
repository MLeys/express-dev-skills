<%- include("../partials/headerNoStyle.ejs") %>
<link rel="stylesheet" href="stylesheets/skills/index.css">
<title>Coding Skills</title>
</head>
<body>

<h1>Skills</h1>
<h4> Updated: <%= time %> </h4>
<ul> 
    <% skills.forEach(function(s) { %>
